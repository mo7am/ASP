    <section id="loginForm" class="section">
        <div style="border: 1px solid black; border-radius: 10px; background-color: #eaeae1" class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    @using (Html.BeginForm("Login", "AccountUser", FormMethod.Post))
                    {
                        @Html.AntiForgeryToken()
                    <center> <h4>Use a local account to log in.</h4></center>
                        <hr />
                        @Html.ValidationSummary(true)
                        <div class="form-group">
                            @Html.LabelFor(m => m.Email, new { @class = "col-md-2 control-label" })
                            <div class="col-md-10">
                                @Html.TextBoxFor(m => m.Email, new { @class = "", @style = "height:30px" })
                                @Html.ValidationMessageFor(m => m.Email)
                            </div>
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.Password, new { @class = "col-md-2 control-label" })
                            <div class="col-md-10">
                                @Html.PasswordFor(m => m.Password, new { @class = "" , id="pass" , @placeholder="enter pass", @style = "height:30px" })
                                <i id="close" style="display:none;" class="icon-eye-close"></i>
                                <i id="open" class="icon-eye-open"></i>
                                @Html.ValidationMessageFor(m => m.Password)

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <div class="checkbox">

                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Log in" class="btn btn-default" />
                            </div>
                        </div>

                    }
                </div>
            </div>
        </div>
    </section>



    <script type="text/javascript">



        $(document).ready(function () {
            //called when key is pressed in textbox
            $("#open").keypress(function (e) {
                $("input").getElementById("pass").attr("type" , "password") 
              


                return false;



            });

        });

    </script>



