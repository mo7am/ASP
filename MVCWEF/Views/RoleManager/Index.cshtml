
@{
    ViewBag.Title = "Index";
}

<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#firstTab">View All</a></li>
    <li hidden="hidden" ><a data-toggle="tab" href="#secondTab"></a></li>
</ul>
<div class="tab-content">
    
    <div id="firstTab" class="tab-pane fade in active">@Html.Action("ViewAll", "RoleManager")</div>
    <div hidden="hidden" id="secondTab" class="tab-pane fade in">@Html.Action("AddOrEdit", "RoleManager")</div>
</div>

<link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" rel="stylesheet" />
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")

    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>


    <script>
        function activatejQueryTable() {
            $("#employeeTable").DataTable();
        }

        $(function () {
            activatejQueryTable();
        });

    </script>
}
