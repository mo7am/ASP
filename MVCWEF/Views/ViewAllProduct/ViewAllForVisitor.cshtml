    <script language="javascript" type="text/javascript">

        function ShowHidePassword(obj) {

            if (obj.checked) {

                document.getElementById('pwd').type = 'text';

            }

            else {

                document.getElementById('pwd').type = 'password';

            }

        }

    </script>
